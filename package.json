{
  "name": "copy-to-clipboard",
  "version": "1.1.1",
  "description": "Copy stuff into clipboard using JS with fallbacks",
  "main": "copy-to-clipboard.cjs.js",
  "browser": "copy-to-clipboard.pack.umd.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel:commonjs": "babel copy-to-clipboard.js --presets=es2015 --plugins=transform-es2015-modules-commonjs --out-file=copy-to-clipboard.cjs.js",
    "webpack:umd": "webpack copy-to-clipboard.cjs.js copy-to-clipboard.pack.umd.js --output-library-target umd",
    "prepublish": "npm run babel:commonjs && npm run webpack:umd" 
  },
  "devDependencies": {
      "babel-cli": "~6.5.0",
      "babel-preset-es2015": "~6.5.0",
      "babel-plugin-transform-es2015-modules-commonjs": "~6.5.0",
      "webpack": "~1.12.13"
  },
  "keywords": [
    "clipboard",
    "copy",
    "browser"
  ],
  "author": "sudodoki <smd.deluzion@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudodoki/copy-to-clipboard"
  },
  "contributors": [
    {
      "name": "Aleksej Shvajka",
      "url": "https://github.com/shvaikalesh"
    }
  ],
  "dependencies": {
    "toggle-selection": "^1.0.3"
  }
}
